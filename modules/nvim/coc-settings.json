{
    "codeLens.enable": true,
    "snippets.userSnippetsDirectory": "~/.config/nvim/snips",
    "languageserver": {
        "terraform": {
            "command": "terraform-ls",
            "args": ["serve"],
            "filetypes": [
                "terraform",
                "tf"
            ],
            "initializationOptions": {},
            "settings": {}
        }
    },
    "signature.floatConfig": {
        "border": true,
        "rounded": true,
        "borderhighlight": "FloatBoarder",
        "highlight": "FloatBoarder",
        "shadow": false,
        "winblend": 15
    },
    "hover.floatConfig": {
        "border": true,
        "rounded": true,
        "borderhighlight": "FloatBoarder",
        "highlight": "FloatBoarder",
        "shadow": false,
        "winblend": 15
    },
    "diagnostic.floatConfig": {
        "border": true,
        "rounded": true,
        "borderhighlight": "FloatBoarder",
        "highlight": "FloatBoarder",
        "shadow": false,
        "winblend": 15
    },
    "suggest.floatConfig": {
        "rounded": true,
        "border": true,
        "borderhighlight": "FloatBoarder",
        "highlight": "FloatBoarder",
        "shadow": false,
        "winblend": 15
    },
    "suggest.noselect": true,
    "suggest.acceptSuggestionOnCommitCharacter": true,
    "suggest.preferCompleteThanJumpPlaceholder": true,
    "suggest.virtualText": true,
    "suggest.selection": "recentlyUsedByPrefix",
    "suggest.defaultSortMethod": "none",
    "suggest.completionItemKindLabels": {
        "method": "  ",
        "function": "  ",
        "variable": "[]",
        "field": "  ",
        "typeParameter": "<>",
        "constant": "  ",
        "class": "  ",
        "interface": "  ",
        "struct": "  ",
        "event": "  ",
        "operator": "  ",
        "module": "  ",
        "property": "  ",
        "enum": "  ",
        "reference": "  ",
        "keyword": "  ",
        "file": "  ",
        "folder": " ﱮ ",
        "color": "  ",
        "unit": "  ",
        "snippet": "  ",
        "text": "  ",
        "constructor": "  ",
        "value": "  ",
        "enumMember": "  "
        }
}
